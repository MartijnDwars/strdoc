  <strategy>
    <name>add-indices</name>
    <description><![CDATA[
                          Adds indices to the elements of a list.
                          The index of the first element is 1.
                          
                          Example: <add-indices> [1,2,3] => [(1,1),(2,2),(3,3)]
                          
                          
                          ]]></description>
    <start>173</start>
    <end>174</end>
    <type><![CDATA[List(a) -> List((Int, a))]]></type>
    
  </strategy>
  
